  withval="$with_kerberos5"
  if test "${with_kerberos5+set}" = set; then
  if test "${with_kerberos+set}" != set; then
    with_kerberos=yes
    cat >> confdefs.h <<\EOF
#define KERBEROS 1
EOF

  fi
fi
cat >> confdefs.h <<\EOF
#define KERBEROS5 1
EOF

fi

